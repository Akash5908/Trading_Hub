generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id         Int      @id @default(autoincrement())
  username   String   @unique
  password   String
  token      String   @unique
  userBalance Int     @default(5000)
}


model Btc_1_min {
  id         Int      @id @default(autoincrement())
  time   Float 
  open  Float
  high  Float
  low   Float
  close Float
}

model Sol_1_min {
  id         Int      @id @default(autoincrement())
  time   Float 
  open  Float
  high  Float
  low   Float
  close Float
}

model Eth_1_min {
  id         Int      @id @default(autoincrement())
  time   Float 
  open  Float
  high  Float
  low   Float
  close Float
}